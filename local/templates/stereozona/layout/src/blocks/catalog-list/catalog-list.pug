        index: 1,
        icon: 'catalog-section-',
        title: 'Портативная аудиотехника',
        main: true
      }).catalog-section--main
      +catalog-section({
        index: 2,
        icon: 'catalog-section-',
        title: 'Акустические системы',
        main: true
      }).catalog-section--main
      +catalog-section({
        index: 3,
        icon: 'catalog-section-',
        title: 'Усилители',
        main: true
      }).catalog-section--main
    .catalog-list__slide
      +catalog-section({
        index: 4,
        icon: 'catalog-section-',
        title: 'Цифро-аналоговые преобразователи',
        main: true
      }).catalog-section--main
      +catalog-section({
        index: 5,
        icon: 'catalog-section-',
        title: 'CD-проигрыватели и транспорты',
        main: true
      }).catalog-section--main
      +catalog-section({
