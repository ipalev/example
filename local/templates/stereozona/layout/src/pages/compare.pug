            .compare__specs-item Транзисторный
          .compare__specs-row(data-index="4")
            .compare__specs-item.compare__specs-item--header Мощность на канал (4 Ом)
            .compare__specs-item 65 Вт
          .compare__specs-row(data-index="5")
            .compare__specs-item.compare__specs-item--header Мощность на канал (8 Ом)
            .compare__specs-item 45 Вт
          .compare__specs-row(data-index="6")
            .compare__specs-item.compare__specs-item--header Встроенный ЦАП
