extends ../templates/template.pug

block content
  div(data-header-classlist="header--white header--border-bottom")

  .compare
    .compare__top
      .compare__top-left
        .container-fluid.px-md-0
          +breadcrumbs({
            links: ['Сравнение товаров']
          })
          h2 Сравнение товаров
          +select({
            placeholder: 'Усилители',
            amount: '3',
            products: [
              { id: 1, name: 'Quad Vena I Play' },
              { id: 2, name: 'Quad Vena II Play' },
              { id: 3, name: 'Quad Vena III Play' }
            ]
          })
          +switch
      .compare__slider-select
        +select({
          placeholder: 'Quad Vena II Play',
        })
        +select({
          placeholder: 'Quad Vena II Play',
        })
        +select({
          placeholder: 'Quad Vena II Play',
        })
        +select({
          placeholder: 'Quad Vena II Play',
        })
      .compare__slider-controls
        .compare__slider-arrow.compare__slider-arrow--prev
        .compare__slider-info
          span.compare__slider-info--current
          span.compare__slider-info--separator -
          span.compare__slider-info--all
        .compare__slider-arrow.compare__slider-arrow--next
      .compare__top-items
        .container-fluid.px-0
          .compare__items-row
            +item({
              title: 'Quad Vena II Play Lancaster Grey',
              picture: 'item-6.png',
              price: '87 989₽',
              modifier: 'compare'
            }).item--compare
            +item({
              title: 'Quad Vena II Play Lancaster Silver',
              picture: 'item-7.png',
              price: '87 989₽',
              modifier: 'compare'
            }).item--compare
            +item({
              title: 'Quad Vena II Lancaster Black',
              picture: 'item-8.png',
              price: '80 289₽',
              modifier: 'compare'
            }).item--compare
            +item({
              title: 'Quad Vena II Lancaster Silver',
              picture: 'item-9.png',
              price: '80 289₽',
              modifier: 'compare'
            }).item--compare
    .compare__specs
